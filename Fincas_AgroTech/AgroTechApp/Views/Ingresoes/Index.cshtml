@model IEnumerable<AgroTechApp.Models.DB.Ingreso>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Monto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Animal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Finca)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RubroIngreso)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Monto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Animal.AnimalId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Finca.FincaId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RubroIngreso.RubroIngresoId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IngresoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IngresoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IngresoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
